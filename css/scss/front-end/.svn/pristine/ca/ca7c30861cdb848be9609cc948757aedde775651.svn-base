/*all*/
html, body, .kmwd-body {
  background-color: #f4f4f4;
  min-height: 100%;
  height: 100%; }

/*header*/
.kmwd-header {
  position: relative;
  text-align: center; }

.kmwd-header .btn-previous {
  left: 0.5em;
  position: absolute;
  width: 1em;
  line-height: 1em;
  height: 1em;
  background-color: transparent;
  font-size: 16px;
  top: 1em; }

.kmwd-header .btn-next {
  right: 0.5em;
  position: absolute;
  width: 1em;
  line-height: 1em;
  height: 1em;
  background-color: transparent;
  font-size: 16px;
  top: 1em; }

/*content*/
.block-tit {
  font-size: 18px;
  line-height: 1em;
  padding-top: 1em;
  padding-bottom: 0.5em;
  padding-left: 16px; }

.block-items {
  font-size: 16px;
  padding-top: 10px;
  padding-bottom: 10px;
  border-top: 1px solid #f2f2f2;
  border-bottom: 1px solid #f2f2f2;
  margin-top: -1px;
  position: relative; }

.block-items.repay-items {
  border-top: none;
  border-bottom: 1px dotted #ffffff; }

.block-items.repay-items:last-child {
  border-bottom: none; }

.block-items::after {
  clear: both;
  content: ' '; }

.block-items label {
  display: inline-block;
  font-size: 14px; }

.repay-total-items {
  border-top: 1px solid #ffffff; }

.item-form-control, .block-items input, .block-items button,
.block-items a.item-button, .block-items select, .panel-items input, .panel-items button.btn-submit-dialog, .circle-items input, .items-verify-code button.btn-submit-dialog {
  font-size: 16px;
  display: inline-block;
  min-width: 50%;
  width: auto;
  float: right;
  border: none;
