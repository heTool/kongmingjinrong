    <!--阳光承保-->
    <div class="text-align-c marg-b-5">
        <img src="http://statics.aijialicai.com.cn/CDN/app/Public/images/kmjrApp/insomnia/insomnia_underwriting.png" width="55%">
    </div>

    <!--friends list-->
    <div class="text-white">
        <h3 class="thumb-header text-align-c">
            <div class="thumb-header-block text-thumb-infop">
                <span>已经有<span class="text-red f20"> 6 </span>个好友领取失眠保险</span><br />
                <span class="f12" data-node="aheadGet">您可提前20分钟赔付</span>

            </div>
        </h3>

        <div data-list="friendList"></div>
        <div class="pm-block-list">
            <div class="pm-block-img">
                <img src="http://www.aijialicai.com.cn/Public/images/kmjrWeb/ico/apple-touch-icon-precomposed.png" class="img-circle marg-t-20">
            </div>
            <div class="f14 pm-block-info text-white lineheight-2">
                <div class="pm-block-info-block clearfix">
                    <div class="pm-block-info-l">如花rtyrt</div>
                    <div class="pm-block-info-r">获得理赔</div>
                </div>
                <div>2015-11-15 24:00</div>
            </div>
            <div class=" pm-block-info2 lineheight-2  text-right f24 text-red">
                5.22元
            </div>
        </div>

        <div class="pm-block-list">
            <div class="pm-block-img">
                <img src="http://www.aijialicai.com.cn/Public/images/kmjrWeb/ico/apple-touch-icon-precomposed.png" class="img-circle marg-t-20">
            </div>
            <div class="f14 pm-block-info text-white lineheight-2">
                <div class="pm-block-info-block clearfix">
                    <div class="pm-block-info-l">貌美如花貌美如花貌美如花rtyrt</div>
                    <div class="pm-block-info-r">获得理赔</div>
                </div>
                <div>2015-11-15 24:00</div>
            </div>
            <div class=" pm-block-info2 lineheight-2  text-right f24 text-red">
                100.22元
            </div>
        </div>


    </div>
</div>

<script>
    (function(){
        $('#insm-mobile-mob').on('click',function(){
            $(this).parent('#insm-mobile').hide();
            $('#insm-ipt').show();
        });

        //好友列表加载
        shareFriendList();
    })(jQuery);

    //ajax加载分享好友
    //@param ajaxUrl：请求链接
    //@param strType：展示类型
    //@param sendData：发送数据
    function shareFriendList(ajaxUrl,strType,sendData){
        var _ajaxUrl = ajaxUrl || 'http://192.168.1.186/kongmingjinrong/html/kmjrApp/temp.json';
        var _strType = strType||2;
        var _sendData = sendData||$('[data-mobile-value]')||[];
        $.ajax({
            url:_ajaxUrl,
            //url:'http://wx.aijialicai.com.cn/insurance/hy_insomnia?mobile=2z2fv7u48cZ4KFZePSNCBO5ebw',
            type:'GET',
            dataType:'json',
            data:'{mobile:_sendData}',
            async:true,
            success:function(data){
                //console.log(data)
                //写入函数
                var _strListAppend = function(headerImg,nickname,createTime,rewardValue){
                    var _strStyle1 = '<div class="thumb-block">';
                    _strStyle1 = _strStyle1 + '<img src="'+headerImg+'" class="img-circle" width="100%">';
                    _strStyle1 = _strStyle1 + '<div class="thumb-block-p">'+nickname+'</div>';
                    _strStyle1 = _strStyle1 + '</div>';

                    var _strStyle2  = '<div class="pm-block-list">';
                        _strStyle2  = _strStyle2 + '<div class="pm-block-img">';
                        _strStyle2  = _strStyle2 + '<img src="'+headerImg+'" class="img-circle marg-t-20">';
                        _strStyle2  = _strStyle2 + '</div>';
                        _strStyle2  = _strStyle2 + '<div class="f12 pm-block-info text-white lineheight-2">';
                        _strStyle2  = _strStyle2 + '<div class="pm-block-info-block clearfix">'+nickname+'</div>';
                        _strStyle2  = _strStyle2 + '<div class="text-nowarp">'+createTime+'</div>';
                        _strStyle2  = _strStyle2 + '</div>';
                        _strStyle2  = _strStyle2 + '<div class=" pm-block-info2">';
                        _strStyle2  = _strStyle2 + '<div class="text-center f12 text-white">获得理赔</div>';
                        _strStyle2  = _strStyle2 + '<div class=" text-center f20 text-red">'+rewardValue+'元</div>';
                        _strStyle2  = _strStyle2 + '</div></div>';

                    if(_strType==1){
                        $('[data-list="friendList"]').append(_strStyle1);
                    }else if(_strType==2){
                        $('[data-list="friendList"]').append(_strStyle2);
                    }
                };

                //如果有就开始写入所有数据
                if(data.list.length>0&&data!='undefined'){
                    for(var i=0; 0<=data.list.length;i++){
                        _strListAppend(data.list[i].wx_headimgurl, data.list[i].wx_nickname, data.list[i].create_time, data.list[i].reward_value);
                    }
                }else{
                    $('[data-node="aheadGet"]').text('不要为了失眠险熬夜哦');
                }


            }
        })
    }



</script>
</body>