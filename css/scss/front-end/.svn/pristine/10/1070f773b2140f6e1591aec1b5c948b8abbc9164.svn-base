.list input.input-inline{ width: inherit !important;}

.list-tab-icon{float: right; padding-right:0.5em;color: #666;line-height: 3.45;}
.list-tab-icon2{ line-height: 2.75 !important;}
body, button, input, select, textarea {
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  -moz-font-feature-settings: "liga","kern";
}
button, select {
  text-transform: none;
}
button, input, optgroup, select, textarea {
  color: inherit;
  font: inherit;
  margin: 0;
}
.am-btn, button, img, input:not([type=radio]):not([type=checkbox]), select {
  vertical-align: middle;
}
*, :after, :before {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}


/*circle loading*/
.progress-loading{ padding: 2em 0;}
.loading-circle{background-color:#ffffff; position:relative;border-radius: 50%; border: 2px #cdcdcd solid; width: 10em;height:10em; margin: auto}
.loading-circle b:before, .loading-circle:after{
    content: '';
    line-height: 100%;
    color: #fffde8;
    text-shadow: 0 1px 2px rgba(0, 0, 0, 0.6);
    position: absolute;
    font-weight: 900;
    left: 1%;
    top: 1%;
    width: 98%;
    height: 98%;
    background-color: #33b5e5;
    border-radius: 50%;
    text-align: center;
    line-height: 50px;
    font-size: 30px;
    box-shadow: 0 0 3px rgba(99, 99, 99, 0.6);
}
.loading-circle:after{
    z-index: 1;
}
.loading-circle b:before{
    z-index: 99;
}
.loading-circle b:after{
    z-index: 999;
}

.loading-circle b{ font-size: 2em;}

.loading-circle b:after{
    line-height: 100%; ;
    color: #ffffff;
    text-shadow: 0 1px 2px rgba(0, 0, 0, 0.6);
    position: absolute;
    font-weight: 900;
    left: 8%;
    top: 8%;
    width: 84%;
    height: 84%;
    background-color: #636363;
    border-radius: 50%;
    text-align: center;
    line-height:3.25em ;
    font-size: 1.25em;
    box-shadow: 0 0 8px rgba(99, 99, 99, 0.9);
}

/*****/
.loading-circle-0 b:after{content:'0%'}
.loading-circle-20 b:after{content:'20%'}
.loading-circle-35 b:after{content:'35%'}
.loading-circle-40 b:after{content:'40%'}
.loading-circle-50 b:after{content:'50%'}
.loading-circle-60 b:after{content:'60%'}
.loading-circle-70 b:after{content:'70%'}
.loading-circle-80 b:after{content:'80%'}
.loading-circle-90 b:after{content:'90%'}
.loading-circle-100 b:after{content:'100%'}

.loading-circle-0 b:before{background-image: linear-gradient(0deg, #fff, #fff);}
.loading-circle-20 b:before{ background-image: linear-gradient(90deg, #fff 50%, transparent 50%, transparent),linear-gradient(-18deg, #ffffff 50%, #33b5e5 50%, #33b5e5);}
.loading-circle-35 b:before{ background-image: linear-gradient(90deg, #fff 50%, transparent 50%, transparent),linear-gradient(36deg, #ffffff 50%, #33b5e5 50%, #33b5e5);}
.loading-circle-40 b:before{ background-image: linear-gradient(90deg, #fff 50%, transparent 50%, transparent),linear-gradient(54deg, #ffffff 50%, #33b5e5 50%, #33b5e5);}
.loading-circle-50 b:before{ background-image: linear-gradient(90deg, #fff 50%, transparent 50%, transparent),linear-gradient(90deg, #ffffff 50%, #33b5e5 50%, #33b5e5);}
.loading-circle-60 b:before{ background-image: linear-gradient(126deg, transparent 50%, #33b5e5 50%, #33b5e5) ,linear-gradient(90deg, #fff 50%, transparent 50%, transparent);}
.loading-circle-70 b:before{ background-image: linear-gradient(162deg, transparent 50%, #33b5e5 50%, #33b5e5) ,linear-gradient(90deg, #fff 50%, transparent 50%, transparent);}
.loading-circle-80 b:before{ background-image: linear-gradient(198deg, transparent 50%, #33b5e5 50%, #33b5e5) ,linear-gradient(90deg, #fff 50%, transparent 50%, transparent);}
.loading-circle-90 b:before{ background-image: linear-gradient(234deg, transparent 50%, #33b5e5 50%, #33b5e5) ,linear-gradient(90deg, #fff 50%, transparent 50%, transparent);}
.loading-circle-100 b:before{}


