/*完善资料*/
.body{position:relative; border-bottom:1px solid #ccc;}
.title{ background:#fff;padding-top:20px; text-align:center;}
.title h2{border-bottom:1px solid #ccc;height:50px; text-align:center;}
.title div{margin:0 auto;}
.on{color:#999;}
.main{background:#FFFFFF;border-bottom:1px solid #eeeeee;margin:15px 0px;padding:0px 10px;}
.main-bg{ margin-top:20px;margin-left:10px; font-size: 80%}
.list{}
