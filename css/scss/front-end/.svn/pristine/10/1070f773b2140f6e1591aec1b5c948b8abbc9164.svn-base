.button a:hover,.button a.active{border-radius:3px;border:1px #0099E5 solid; padding:10px 0;-webkit-box-flex:1;-webkit-flex:1; background-color:#0099E5;color:#fff; text-decoration:none;}
.button-l{ width:45% !important; float: left;}
.button-r{ width: 45%  !important; float: right;}
.title_jg{line-height:1.8em; padding: 1em;color:red; background:#FFF391;border-top:1px solid #ccc; border-bottom:1px solid #ccc; text-align:center; font-size: 10px}


/*进度条*/

.progress {
  padding:20px 20px 20px 20px;
  border-left: 1px solid transparent;
  border-right: 1px solid transparent;
  border-radius: 10px;
}
.progress-bar{position: relative}
.progress-bar-l{position:absolute; left: 0; top: 0; font-size:1em}
.progress-bar-m{position:absolute; left: 0; top: 6px; margin: 0 10% 0 10% !important; background: #f0f0f0; width: 78%; height: 10px; overflow:hidden}
.progress-bar-load{background-color: #0099E5; width:0; margin: 0 !important;}
.progress-bar-r{position:absolute; right: -0.5em; top: 0;font-size:1em}

.progress > span.progress-bar-num {
  padding: 0 8px;
  font-size: 1em;
  /*font-weight: bold;*/
  color: #404040;
  color: rgba(0, 0, 0, 0.7);
  text-shadow: 0 1px rgba(255, 255, 255, 0.4);
}

.progress-bar{
  height:3em;
  width: 100%;
  background: white;
}

.msgContact{ padding-top: 5em; padding-bottom: 1em}

/*贷款协议*/
.loanBody{ line-height: 1.8; padding: 1em;  font-size: 0.8em;}
.loanBody h1,.loanBody h2,.loanBody h3{ line-height: 3; font-weight: bold; word-break: keep-all; white-space: nowrap}
.loanBody h1{ font-size: 1.25em;;text-align: center; word-break: keep-all; white-space: nowrap;}
.loanBody h2{ font-size:1.2em}
.loanBody h3{ font-size:1.15em}
.loanBody p{padding:0.2em 0;}
.loanBody table{ line-height: 1.4em; border-top: 1px solid #ccc; border-left: 1px solid #ccc; }
.loanBody td{ padding:0.2em 0.5em;line-height: 1.4em;border-right: 1px solid #ccc;border-bottom: 1px solid #ccc;}
.text-p{ padding-left: 2em !important;}
.loanNotice{ color:darkred; font-weight: bold; margin-top: 2em}
.loanNoticeCont{color:darkred; font-weight: bold; }
.loanTr{text-align: right}
.loanEnd{ margin-top: 2em}

.loanList{  padding: 1em 0;}
.loanList li{ border-top:1px solid #ccc;border-bottom:1px solid #ccc;margin: 0.45em 0; list-style: none; padding:0.75em 0.5em 0.75em 2.5em; background:#fff url('../../images/step/icon-loan-list.gif') 0.5em center no-repeat; background-size: 20px; }
.loanList li a{ display: block; text-decoration: none; color: #000; background: url('../../images/step/icon-loan-lt.gif') right center no-repeat; background-size:20px}
.loanList li a span{ float: right}

